<!DOCTYPE html>
<html>
  <head>
    <title>RailsJsDemo</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'app', 'data-turbolinks-eval': false %>
  </head>

  <body data-route="<%= script_route %>">
    <%= yield %>
  </body>
</html>
